{
  "name": "cryptotao_test_code",
  "version": "1.0.0",
  "description": "An application code for CryptoTao made by iborba",
  "main": "app.js",
  "scripts": {
    "build": "tsc -p",
    "start": "npm run build && node build/server.js",
    "start:dev": "nodemon",
    "prepare": "husky install",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "jest --clearCache && jest -c jest-unit-config.js",
    "test:watch": "jest --clearCache && jest __tests__/.*/*.spec.ts --watch --detectOpenHandles --maxWorkers=1 -c jest-unit-config.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint:fix"
  },
  "author": "https://github.com/iborba",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@shelf/jest-mongodb": "^2.2.1",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.4.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^17.0.25",
    "@types/sequelize": "^4.28.11",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@nfteyez/sol-rayz": "^0.10.2",
    "@solana/web3.js": "^1.39.1",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.4",
    "cors": "^2.8.5",
    "editorconfig": "^0.15.3",
    "express": "^4.17.3",
    "http-status-codes": "^2.2.0",
    "mongodb": "^4.5.0",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.0.5",
    "uuid": "^8.3.2"
  }
}
